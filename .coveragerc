[run]
branch = True
source = djangocms_time_wizard
omit =
    djangocms_time_wizard/migrations/*
    djangocms_time_wizard/workarounds.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
ignore_errors = True
